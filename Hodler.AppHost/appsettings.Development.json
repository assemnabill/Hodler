{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Aspire": {
    "Npgsql": {
      "EntityFrameworkCore": {
        "PostgreSQL": {
          "ConnectionString": "Host=postgres;Database=hodler-db",
          "DbContextPooling": true,
          "DisableHealthChecks": true,
          "DisableTracing": true,
          "UserDbContext": {
            "ConnectionString": "Host=postgres;Database=hodler-db",
            "DisableTracing": false
          }
        }
      }
    }
  }
}
